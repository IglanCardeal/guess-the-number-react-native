{
  "expo": {
    "name": "guess-number-app",
    "slug": "guess-number-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/guess.png",
    "splash": {
      "image": "./assets/guess.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/guess.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/guess.png"
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive"
    }
  }
}